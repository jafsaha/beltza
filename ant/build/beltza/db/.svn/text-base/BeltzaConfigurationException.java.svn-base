package beltza.db;

public class BeltzaConfigurationException extends RuntimeException {
	
	
	public BeltzaConfigurationException() {
		super();
	}

	public BeltzaConfigurationException(String message) {
		super(message);
	}

	public BeltzaConfigurationException(String message, Throwable cause) {
		super(message, cause);
	}
	
	public BeltzaConfigurationException(Throwable cause) {
		super(cause);
	}
	
	
}
