package beltza.util;

import beltza.commons.Operator;

public class OperatorConverter {

	public static String convert(Operator operator) {
		if (operator == null) {
			return "=";
		}
		if (operator.name() == Operator.IGUAL.name()) {
			return "=";
		} else if (operator.name() == Operator.MAYOR_IGUAL.name()) {
			return ">=";
		} else if (operator.name() == Operator.MENOR_IGUAL.name()) {
			return "<=";
		}
		return "=";
		
	}
	
	
	
	
}
